<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/aymhomes"
    tools:context=".MainActivity">


    <ImageView
        android:layout_width="264dp"
        android:layout_height="241dp"
        android:layout_marginStart="79dp"
        android:layout_marginTop="156dp"
        android:layout_marginEnd="68dp"
        android:background="@drawable/homes_logo"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="59dp"
        android:layout_height="24dp"
        android:layout_marginStart="251dp"
        android:layout_marginTop="410dp"
        android:layout_marginEnd="101dp"
        android:text="LOGIN"
        android:textColor="@color/burntOrange"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="88dp"
        android:layout_height="23dp"
        android:layout_marginStart="208dp"
        android:layout_marginTop="466dp"
        android:layout_marginEnd="115dp"
        android:text="REGISTER"
        android:textColor="@color/burntOrange"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="165dp"
        android:layout_height="28dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="527dp"
        android:layout_marginEnd="123dp"
        android:text="FORGOT PASSWORD"
        android:textColor="@color/burntOrange"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="181dp"
        android:layout_height="27dp"
        android:layout_marginStart="107dp"
        android:layout_marginTop="587dp"
        android:layout_marginEnd="123dp"
        android:text="CONTINUE AS GUEST"
        android:textColor="@color/burntOrange"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/loginBTN"
        android:layout_width="54dp"
        android:layout_height="35dp"
        android:layout_marginStart="322dp"
        android:layout_marginTop="404dp"
        android:layout_marginEnd="35dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_login_24" />

    <ImageButton
        android:id="@+id/registerBTN"
        android:layout_width="58dp"
        android:layout_height="35dp"
        android:layout_marginStart="310dp"
        android:layout_marginTop="466dp"
        android:layout_marginEnd="43dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_app_registration_24" />

    <ImageButton
        android:id="@+id/forgotPass_BTN"
        android:layout_width="55dp"
        android:layout_height="33dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="527dp"
        android:layout_marginEnd="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_email_24" />

    <ImageButton
        android:id="@+id/guestBTN"
        android:layout_width="40dp"
        android:layout_height="38dp"
        android:layout_marginStart="296dp"
        android:layout_marginTop="576dp"
        android:layout_marginEnd="75dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_device_unknown_24" />


</androidx.constraintlayout.widget.ConstraintLayout>